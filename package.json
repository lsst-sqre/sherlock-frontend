    "swr": "^1.2.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
